config {
    ...issko_config.settings.occ_days_o_temp
}

    SELECT 
        *,
        -- Calculate the specific calendar month for this period (1..15)
        -- occ=1 is current month, occ=2 is next month, etc.
        DATE_ADD(dat_mois, INTERVAL (occ - 1) MONTH) AS month_actual
    FROM ${ref("occ_rows")}